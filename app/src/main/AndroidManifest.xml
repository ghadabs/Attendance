<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.Attendance.AttendanceApp">
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Attendance App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.Attendance.AttendanceApp.Reports_Detail_Activity"
            android:theme="@style/AppTheme2" />
        <activity android:name="com.Attendance.AttendanceApp.Reports_Activity" />
        <activity android:name="com.Attendance.AttendanceApp.Register" />
        <activity android:name="com.Attendance.AttendanceApp.MainActivity" />
        <activity
            android:name="com.Attendance.AttendanceApp.ClassDetail_Activity"
            android:theme="@style/FullScreenTheme" />
        <activity
            android:name="com.Attendance.AttendanceApp.Insert_class_Activity"
            android:theme="@style/AppTheme2" />
        <activity android:name="com.Attendance.AttendanceApp.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>